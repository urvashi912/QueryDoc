{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/upload",
        "dest": "/server.js",
        "methods": ["POST"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      },
      {
        "src": "/ask",
        "dest": "/server.js",
        "methods": ["POST"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }
  