{
    "version": 2,
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/upload",
            "dest": "/backend/server.js",
            "methods": ["POST"],
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/ask",
            "dest": "/backend/server.js",
            "methods": ["POST"],
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/",
            "dest": "/server.js",
            "methods": ["GET"],
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
        }
    ], "builds": [
        {
          "src": "api/server.js",
          "use": "@vercel/node"
        }
      ]
}
